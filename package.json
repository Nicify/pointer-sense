{
  "name": "sensi-mouse",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "format:ts": "prettier --loglevel warn --cache --write \"src/**/*.@(html|js|ts|tsx|css|scss|md)\"",
    "lint:ts": "tsc --noEmit && eslint src",
    "format:rs": "cd src-tauri && cargo fmt",
    "lint:rs": "cd src-tauri && cargo clippy",
    "format": "npm run format:ts && npm run format:rs",
    "lint": "npm run lint:ts && npm run lint:rs",
    "clean": "rm -rf node_modules pnpm-lock.yaml && cd src-tauri && cargo clean"
  },
  "dependencies": {
    "@mantine/core": "6.0.5",
    "@mantine/hooks": "6.0.5",
    "@mantine/utils": "6.0.5",
    "@stitches/react": "1.2.8",
    "@swan-io/boxed": "0.12.1",
    "@swan-io/chicane": "1.3.4",
    "@tauri-apps/api": "1.2.0",
    "@total-typescript/ts-reset": "0.4.2",
    "jotai": "2.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-use": "17.4.0",
    "ress": "5.0.2",
    "ts-pattern": "4.2.2"
  },
  "devDependencies": {
    "@babel/core": "7.21.4",
    "@tauri-apps/cli": "1.2.3",
    "@types/node": "18.15.11",
    "@types/react": "18.0.31",
    "@types/react-dom": "18.0.11",
    "@vitejs/plugin-react": "3.1.0",
    "core-dts": "0.0.3",
    "eslint": "8.37.0",
    "eslint-config-with-tsconfig": "1.2.5",
    "prettier": "2.8.7",
    "sass": "1.60.0",
    "typescript": "5.0.3",
    "vite": "4.2.1"
  },
  "packageManager": "pnpm@8.1.0"
}