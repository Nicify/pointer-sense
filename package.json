{
    "name": "sensi-mouse",
    "version": "1.0.9",
    "type": "module",
    "scripts": {
        "preinstall": "npx only-allow pnpm",
        "build": "typesafe-i18n --no-watch && tsc && vite build",
        "clean": "rm -rf dist node_modules pnpm-lock.yaml && cd src-tauri && cargo clean",
        "dev": "vite",
        "format": "prettier --cache --write \"src/**/*.@(html|js|ts|tsx|css|scss|md)\" && cd src-tauri && cargo fmt",
        "i18n": "typesafe-i18n",
        "lint": "tsc --noEmit && eslint src --fix && cd src-tauri && cargo clippy",
        "preview": "vite preview",
        "tauri": "tauri",
        "update-version": "tsx ./scripts/update_version.ts"
    },
    "dependencies": {
        "@mantine/core": "6.0.10",
        "@mantine/hooks": "6.0.10",
        "@mantine/utils": "6.0.10",
        "@stitches/react": "1.2.8",
        "@swan-io/boxed": "1.0.1",
        "@swan-io/chicane": "1.3.4",
        "@tauri-apps/api": "1.3.0",
        "@total-typescript/ts-reset": "0.4.2",
        "jotai": "^2.1.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-use": "17.4.0",
        "ress": "5.0.2",
        "tauri-plugin-autostart-api": "git://github.com/tauri-apps/tauri-plugin-autostart#0c8e3ad",
        "tauri-plugin-store-api": "git://github.com/tauri-apps/tauri-plugin-store#f0da2d1",
        "throttle-debounce": "5.0.0",
        "tiny-invariant": "1.3.1",
        "ts-pattern": "4.2.3",
        "zod": "3.21.4"
    },
    "devDependencies": {
        "@babel/core": "7.21.8",
        "@tauri-apps/cli": "1.3.1",
        "@types/node": "20.1.0",
        "@types/react": "18.2.6",
        "@types/react-dom": "18.2.4",
        "@types/throttle-debounce": "5.0.0",
        "@vitejs/plugin-react-swc": "3.3.1",
        "core-dts": "0.0.3",
        "eslint": "8.40.0",
        "eslint-config-with-tsconfig": "1.4.6",
        "prettier": "2.8.8",
        "sass": "1.62.1",
        "ts-essentials": "9.3.2",
        "tsx": "3.12.7",
        "typesafe-i18n": "5.24.3",
        "typescript": "5.1.0-dev.20230508",
        "vite": "4.3.5",
        "zx": "7.2.2"
    },
    "pnpm": {
        "overrides": {
            "react": "18.2.0",
            "react-dom": "18.2.0",
            "esbuild": "0.17.17",
            "typescript": "5.1.0-dev.20230508"
        }
    }
}