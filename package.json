{
    "name": "sensi-mouse",
    "version": "1.0.7",
    "type": "module",
    "scripts": {
        "preinstall": "npx only-allow pnpm",
        "build": "typesafe-i18n --no-watch && tsc && vite build",
        "clean": "rm -rf dist node_modules pnpm-lock.yaml && cd src-tauri && cargo clean",
        "dev": "vite",
        "format:ts": "prettier --cache --write \"src/**/*.@(html|js|ts|tsx|css|scss|md)\"",
        "format:rs": "cd src-tauri && cargo fmt",
        "i18n": "typesafe-i18n",
        "lint": "pnpm run lint:ts && pnpm run lint:rs",
        "lint:rs": "cd src-tauri && cargo clippy",
        "lint:ts": "tsc --noEmit && eslint src --fix",
        "preview": "vite preview",
        "tauri": "tauri",
        "update-version": "tsx ./scripts/update_version.ts"
    },
    "dependencies": {
        "@mantine/core": "6.0.8",
        "@mantine/hooks": "6.0.8",
        "@mantine/utils": "6.0.8",
        "@stitches/react": "1.2.8",
        "@swan-io/boxed": "0.13.0",
        "@swan-io/chicane": "1.3.4",
        "@tauri-apps/api": "1.2.0",
        "@total-typescript/ts-reset": "0.4.2",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-use": "17.4.0",
        "ress": "5.0.2",
        "tauri-plugin-autostart-api": "git://github.com/tauri-apps/tauri-plugin-autostart#0c8e3ad",
        "tauri-plugin-store-api": "git://github.com/tauri-apps/tauri-plugin-store#f0da2d1",
        "throttle-debounce": "5.0.0",
        "ts-pattern": "4.2.2",
        "valtio": "1.10.4",
        "zod": "3.21.4"
    },
    "devDependencies": {
        "@babel/core": "7.21.4",
        "@tauri-apps/cli": "1.2.3",
        "@types/node": "18.15.12",
        "@types/react": "18.0.37",
        "@types/react-dom": "18.0.11",
        "@types/throttle-debounce": "5.0.0",
        "@vitejs/plugin-react": "3.1.0",
        "core-dts": "0.0.3",
        "eslint": "8.38.0",
        "eslint-config-with-tsconfig": "1.3.7",
        "prettier": "2.8.7",
        "sass": "1.62.0",
        "tsx": "3.12.6",
        "typesafe-i18n": "5.24.3",
        "typescript": "5.1.0-beta",
        "vite": "4.3.1",
        "zx": "7.2.1"
    },
    "pnpm": {
        "overrides": {
            "react": "18.2.0",
            "react-dom": "18.2.0",
            "esbuild": "0.17.17",
            "typescript": "5.1.0-beta"
        }
    }
}
