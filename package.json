{
    "name": "sensi-mouse",
    "version": "1.0.10",
    "type": "module",
    "scripts": {
        "preinstall": "npx only-allow pnpm",
        "build": "typesafe-i18n --no-watch && tsc && vite build",
        "clean": "rm -rf dist node_modules pnpm-lock.yaml && cd src-tauri && cargo clean",
        "dev": "vite",
        "format": "rome format . --write && cd src-tauri && cargo fmt",
        "i18n": "typesafe-i18n",
        "lint": "tsc --noEmit && eslint src --fix && cd src-tauri && cargo clippy",
        "preview": "vite preview",
        "tauri": "tauri",
        "update-version": "tsx ./scripts/update_version.ts"
    },
    "dependencies": {
        "@mantine/core": "6.0.13",
        "@mantine/hooks": "6.0.13",
        "@mantine/utils": "6.0.13",
        "@swan-io/chicane": "1.4.0",
        "@tauri-apps/api": "1.3.0",
        "@total-typescript/ts-reset": "0.4.2",
        "ftld": "0.57.14",
        "immer": "10.0.2",
        "jotai": "2.1.0",
        "jotai-immer": "0.2.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-use": "17.4.0",
        "ress": "5.0.2",
        "tauri-plugin-autostart-api": "git://github.com/tauri-apps/tauri-plugin-autostart#0c8e3ad",
        "tauri-plugin-store-api": "git://github.com/tauri-apps/tauri-plugin-store#f0da2d1",
        "throttle-debounce": "5.0.0",
        "tiny-invariant": "1.3.1",
        "ts-pattern": "4.3.0",
        "zod": "3.21.4"
    },
    "devDependencies": {
        "@tauri-apps/cli": "1.3.1",
        "@types/node": "20.2.5",
        "@types/react": "18.2.8",
        "@types/react-dom": "18.2.4",
        "@types/throttle-debounce": "5.0.0",
        "@vanilla-extract/css": "1.11.0",
        "@vanilla-extract/vite-plugin": "3.8.2",
        "@vitejs/plugin-react-swc": "3.3.1",
        "core-dts": "0.0.3",
        "eslint": "8.42.0",
        "eslint-config-with-tsconfig": "1.5.5",
        "rome": "12.1.3",
        "sass": "1.62.1",
        "ts-essentials": "9.3.2",
        "tsx": "3.12.7",
        "typesafe-i18n": "5.24.3",
        "typescript": "5.1.3",
        "vite": "4.3.9",
        "zx": "7.2.2"
    },
    "//": "dedupe packages for pnpm",
    "pnpm": {
        "overrides": {
            "immer": "10.0.2",
            "react": "18.2.0",
            "react-dom": "18.2.0",
            "@mantine/core": "6.0.11",
            "@mantine/hooks": "6.0.11",
            "@mantine/utils": "6.0.11",
            "esbuild": "0.17.17",
            "typescript": "5.1.3"
        }
    }
}
